{
  "name": "biyom-code-manager",
  "displayName": "Biyom code manager",
  "author": "Xavi PÃ©rez Fondevila",
  "publisher": "XaviPerez",
  "description": "Extension for beyond enjoyers",
  "repository": {
    "type": "git",
    "url": "https://github.com/XavierPerezFondevila/biyom-code-manager"
  },
  "icon": "images/appIcon.png",
  "version": "1.3.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
			"activitybar": [
				{
					"id": "package-explorer",
					"title": "Biyom code manager",
					"icon": "images/tomatrial.svg"
				}
			]
		},
		"views": {
			"package-explorer": [
				{
					"id": "extensionDependencies",
					"name": "Commands sheet",
					"contextualTitle": "Biyom code manager"
				}
			]
		},
    "commands": [
      {
        "command": "biyom-code-manager.createInternal",
        "title": "Create Internal Controller"
      },
      {
        "command": "biyom-code-manager.createDocumentsScript",
        "title": "Create Document Script",
        "argument": "create"
      },
      {
        "command": "biyom-code-manager.getDocumentsScript",
        "title": "Import Document Script",
        "arguments": "import"
      },
      {
        "command": "biyom-code-manager.importDynamicForms",
        "title": "Import Dynamic Forms"
      },
      {
        "command": "biyom-code-manager.importLibrary",
        "title": "Import Library"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "path": "^0.12.7"
  }
}
